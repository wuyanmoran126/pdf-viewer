{
  "version": 2,
  "builds": [
    {
      "src": "api/upload.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/admin/upload",
      "dest": "/api/upload.js",
      "methods": ["POST"]
    },
    {
      "src": "/admin",
      "dest": "/public/admin/index.html"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}
